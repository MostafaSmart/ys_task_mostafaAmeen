<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#020101" />
            <corners android:radius="15dp" />
            <stroke android:width="3dp" android:color="#FBFBFB" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/lite_blue" />
            <corners android:radius="15dp" />
            <stroke android:width="3dp" android:color="@color/primmery" />
        </shape>
    </item>
</selector>
